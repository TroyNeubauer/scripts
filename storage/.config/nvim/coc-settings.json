{
    "java.home": "/usr/lib/jvm/java-11-openjdk",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/java-8-amazon-corretto",
            "default": true
        }
    ],
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp"
            ],
            "rootPatterns": [
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        }
    },
    "rust-analyzer.enable": true,
    "rust-analyzer.updates.prompt": false,
    "cSpell.userWords": [
      "API's",
      "GCed",
      "Gitflow",
      "Hasher",
      "Inet",
      "Keepalive",
      "Neubauer",
      "POSTROUTING",
      "PRNG",
      "RC's",
      "Seedable",
      "Tunn",
      "VPNing",
      "Zeroizing",
      "backends",
      "bindgen",
      "boringtun",
      "bprev",
      "censorbuster",
      "chacha",
      "chrono",
      "concat",
      "config's",
      "datagram",
      "decapsulated",
      "farings",
      "gc's",
      "genkey",
      "geteuid",
      "handshaken",
      "ipaddr",
      "ipnet",
      "libc",
      "markdownlint",
      "nanos",
      "netlink",
      "nullblack",
      "oneshot",
      "peroidocly",
      "popen",
      "println",
      "pubkey",
      "recvfrom",
      "repr",
      "reqwest",
      "rsplit",
      "sendto",
      "setconf",
      "smallvec",
      "sout",
      "syncconf",
      "synchapi",
      "tempdir",
      "uapi",
      "widestring",
      "winapi",
      "wintun",
      "wireguard's",
      "xsel",
      "zeroize"
    ],
    "markdownlint.config": {
        "default": true,
        "line-length": false
    }
}
